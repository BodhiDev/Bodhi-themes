	fg[SELECTED]      = @selected_fg_color
	fg[INSENSITIVE]   = @d_text_color

	bg[NORMAL]        = @bg_color
	bg[PRELIGHT]      = shade (0.8, @bg_color)
	bg[ACTIVE]        = shade (0.8, @bg_color)
	bg[SELECTED]	  = @selected_bg_color
	bg[INSENSITIVE]   = @bg_color1
        #bg_pixmap[NORMAL] = "/Others/test-image.png"

	base[NORMAL]      = @base_color
	base[PRELIGHT]    = shade (0.95, @bg_color)
	base[ACTIVE]      = mix (0.7, @selected_bg_color, @bg_color)
	base[SELECTED]    = @selected_bg_color
	base[INSENSITIVE] = @d_text_color

	text[NORMAL]      = @text_color
	text[PRELIGHT]    = @text_color
	text[ACTIVE]      = @selected_fg_color
	text[SELECTED]    = @selected_fg_color
	text[INSENSITIVE] = @d_text_color


  engine "pixmap"
  {
  

    
